{
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-constant-elements",
        "@babel/plugin-transform-runtime"
      ]
    }
  },
  "presets": [
    ["@babel/preset-env", {
      "targets": { "browsers": [
        ">0.25%",
        "not ie 11",
        "not op_mini all"
      ] }
    }],
    "@babel/preset-react"
  ]
}
